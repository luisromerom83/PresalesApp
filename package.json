{
  "name": "presales",
  "productName": "Presales LatinTool",
  "version": "1.1.8",
  "description": "Presales LatinTool - Productivity tool for Presales Engineers",
  "main": "main.js",
  "author": "Presales Latin America Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/luisromerom83/PresalesApp.git"
  },
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "dist": "electron-builder",
    "lint": "echo \"No linting configured\""
  },
  "build": {
    "appId": "com.presales.manager",
    "productName": "Presales LatinTool",
    "directories": {
      "output": "dist/v1.1.8"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "artifactName": "PresalesApp.${ext}"
    },
    "files": [
      "**/*",
      "!dist/*",
      "!dist/**/*"
    ],
    "cscLink": null,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": [
      {
        "provider": "github",
        "owner": "luisromerom83",
        "repo": "PresalesApp"
      }
    ]
  },
  "keywords": [],
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "electron": "^33.3.0",
    "electron-builder": "^26.7.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.7.3",
    "papaparse": "^5.5.3"
  }
}